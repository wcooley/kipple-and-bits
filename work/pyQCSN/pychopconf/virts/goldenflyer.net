<VirtualHost 198.145.93.8 65.201.55.8>
        ServerName goldenflyer.net
        ServerAlias *.goldenflyer.net
        ServerAdmin webmaster@goldenflyer.net
        DocumentRoot /home/httpd/virtual//goldenflyer.net/docs/
        ErrorLog  /home/httpd/virtual//goldenflyer.net/logs/error
        CustomLog /home/httpd/virtual//goldenflyer.net/logs/access combined
        ScriptLog /home/httpd/virtual//goldenflyer.net/logs/script
        ScriptAlias /cgi-bin/ /home/httpd/virtual//goldenflyer.net/cgi-bin/
        ScriptAlias /sys-bin/ /home/httpd/cgi-bin/
        Alias /stats/ /home/httpd/virtual//goldenflyer.net/stats/
	<IfModule mod_bandwidth.c>
        	BandWidthModule on
        	BandWidth all 16384
		MinBandwidth all 8192
	</IfModule>

</Virtualhost>
	