<VirtualHost 198.145.93.8 65.201.55.8>
	ServerAdmin webmaster@qcsn.com
	DocumentRoot /home/httpd/virtual/sandy
	ServerName sandy.or.us
	ServerAlias *.sandy.or.us
	ServerAlias www.ci.sandy.or.us
	ErrorLog /var/log/httpd/sandy.or.us/error
	CustomLog /var/log/httpd/sandy.or.us/access combined
	AddType application/x-httpd-cgi .cgi
	ScriptAlias /cgi-bin/ /home/httpd/virtual/sandy/cgi-bin/
	ScriptAlias /syscgi/ /home/httpd/cgi-bin/
	ScriptAlias /sys-bin/ /home/httpd/cgi-bin/
</VirtualHost>
	